CONFIG_TOUCHSCREEN_SYNAPTICS_DSX_E7S_I2C=y
CONFIG_TOUCHSCREEN_SYNAPTICS_DSX_E7S_CORE=y
CONFIG_TOUCHSCREEN_SYNAPTICS_DSX_E7S_RMI_DEV=y
CONFIG_TOUCHSCREEN_SYNAPTICS_DSX_E7S_FW_UPDATE=y
CONFIG_TOUCHSCREEN_SYNAPTICS_DSX_E7S_GESTURE=y
CONFIG_TOUCHSCREEN_SYNAPTICS_DSX_E7S_TEST_REPORTING=y
